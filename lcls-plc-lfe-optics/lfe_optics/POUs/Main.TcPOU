    fbMR2L0_States: FB_Coatings_States:= ( 
			bEnable:= TRUE , 
			stTransitionBeam:= PMPS_GVL.cst0RateBeam , 
			nTransitionAssertionID:= 16#C21 , 
			nUnknownAssertionID:= 16#C20 , 
			sB4CType:= 'B4C' , 
			nB4CPosition:= 7018200 , 
			fB4CDelta:= 1000 , 
			nB4CCoatingBitmask:= 2#0000_0011_1111_1111, 
