        pv: MR1L0:HOMS:COATING:STATE:SET
        io: io
    '}
    MR1L0_Coating_enumSet: ENUM_Coating_States;
	 {attribute 'pytmc' := '
        pv: MR1L0:HOMS:COATING:STATE;
        io: io;
		field: ZRST Unknown ;
		field: ONST B4C;
		field: TWST B4C_Ni ;
    '}
